    "description": "LSCache Implementation for Laravel",
    "require": {
        "laravel/framework": "^5.1|^6|^7"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Lucas Rolff",
            "email": "lrolff@litespeedtech.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
